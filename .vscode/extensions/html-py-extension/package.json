{
  "name": "html-py",
  "displayName": "HTML-PY",
  "description": "HTML-in-Python language features for embedded templates",
  "version": "0.0.1",
  "publisher": "html-py",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "html-py",
        "aliases": [
          "HTML-PY",
          "html-py"
        ],
        "extensions": [
          ".fpy",
          ".pyhtml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "html-py",
        "scopeName": "source.html-py",
        "path": "./syntaxes/html-py.tmLanguage.json",
        "injectTo": [
          "source.python"
        ],
        "embeddedLanguages": {
          "meta.embedded.inline.html": "html",
          "meta.embedded.inline.css": "css",
          "meta.embedded.inline.js": "javascript"
        }
      }
    ],
    "configurationDefaults": {
      "emmet.includeLanguages": {
        "html-py": "html",
        "python": "html"
      }
    }
  }
}
